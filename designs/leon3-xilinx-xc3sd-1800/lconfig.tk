	$w.config.f.x5.x.menu add radiobutton -label "2" -variable tmpvar_26 -value "2" -command "update_active"
	$w.config.f.x5.x.menu add radiobutton -label "4" -variable tmpvar_26 -value "4" -command "update_active"
	$w.config.f.x5.x.menu add radiobutton -label "8" -variable tmpvar_26 -value "8" -command "update_active"
	$w.config.f.x5.x.menu add radiobutton -label "16" -variable tmpvar_26 -value "16" -command "update_active"
	menusplit $w $w.config.f.x5.x.menu 5
	bool $w.config.f 8 6 "Enable AHB trace buffer filters " CONFIG_DSU_AFILT
	bool $w.config.f 8 7 "Enable statistics outputs       " CONFIG_DSU_ASTAT
	bool $w.config.f 8 8 "Enable LEON3 Statistics Module       " CONFIG_STAT_ENABLE
	int $w.config.f 8 9 "Number of counters (1 - 64)           " CONFIG_STAT_CNT
	int $w.config.f 8 10 "Number of accumulative counters       " CONFIG_STAT_NMAX



	focus $w
	update_active
	global winx; global winy
	if {[winfo exists .menu3] == 0} then {menu3 .menu3 "Processor            "}
	set winx [expr [winfo x .menu3]+30]; set winy [expr [winfo y .menu3]+30]
	if {[winfo exists $w]} then {wm geometry $w +$winx+$winy}
	update idletasks
	if {[winfo exists $w]} then  {$w.config.canvas create window 0 0 -anchor nw -window $w.config.f

	$w.config.canvas configure \
		-width [expr [winfo reqwidth $w.config.f] + 1]\
		-scrollregion "-1 -1 [expr [winfo reqwidth $w.config.f] + 1] \
			 [expr [winfo reqheight $w.config.f] + 1]"

	set winy [expr [winfo reqh $w] - [winfo reqh $w.config.canvas]]
	set scry [expr [winfo screenh $w] / 2]
	set maxy [expr [winfo screenh $w] * 3 / 4]
